{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "hostRequirements": {
    "cpus": 4
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "npm install",
  "postCreateCommand": "",
  "postAttachCommand": {
    "server": "npm start"
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "src/App.jsx"
      ]
    },
    "vscode": {
      "extensions": [
        "AvinashMaheshwari.projexity-ai" 
      ],
      "settings": {
        "workbench.activityBar.visible": true,
        "workbench.sideBar.location": "left",
        "workbench.startupEditor": "none",
        "workbench.view.extension.AvinashMaheshwari-projexity-ai": true
      }
    }
  },
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [3000],
  "remoteEnv": {
    "REPO_ID": "871482895"
  }
}
